{
  "name": "cardano-starter",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev",
    "test": "turbo test",
    "lint": "turbo lint",
    "setup:container": "bash -lc 'set -e; (command -v corepack >/dev/null 2>&1 && corepack enable && corepack prepare pnpm@9 --activate) || true; pnpm -v || true; pnpm install || true; bash .devcontainer/scripts/install-aiken.sh'",
    "infra:up": "pnpm --filter @starter/dolos infra:up",
    "infra:down": "pnpm --filter @starter/dolos infra:down",
    "infra:logs": "pnpm --filter @starter/dolos infra:logs"
  },
  "devDependencies": {
    "turbo": "^2.1.0",
    "typescript": "^5.4.0"
  },
  "dependencies": {
    "express-rate-limit": "7",
    "zod": "^4.1.12"
  }
}
